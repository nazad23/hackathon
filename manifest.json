{
    "manifest_version": 2,
    "name": "Attune",
    "description": "An extension that allows you to pause, play, and change the media you have opened in other tabs!",
    "version": "1.0",
    "browser_action": {
        "default_icon": "img/apple2.png",
        "default_popup": "popup.html"
    },
    // "permissions": [
    //     "activeTab"
    // ],
    "contentScripts" : [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["popup.js", "content.js", "jquery-2.1.3.min.js"]
       }
      ],
      "background": {
        "scripts": ["background.js"]
      }
}